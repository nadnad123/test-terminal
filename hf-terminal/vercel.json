{
    "version": 2,
    "builds": [
        {
            "src": "api/main.go",
            "use": "@vercel/go"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.go"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}
